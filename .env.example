# MongoDB 連接字串
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&appName=AppName

# 開發環境設定
NODE_ENV=development

# LINE Login Channel 設定
LINE_LOGIN_CHANNEL_ID=your_login_channel_id
LINE_LOGIN_CHANNEL_SECRET=your_login_channel_secret

# LINE Messaging API Channel 設定
LINE_CHANNEL_ID=your_messaging_channel_id
LINE_CHANNEL_SECRET=your_messaging_channel_secret
LINE_CHANNEL_ACCESS_TOKEN=your_channel_access_token

# LINE LIFF App ID
LIFF_ID=your_liff_id
NEXT_PUBLIC_LIFF_ID=your_liff_id

# Vercel 部署設定
VERCEL_URL=your_vercel_domain
NEXTAUTH_URL=https://your_vercel_domain
